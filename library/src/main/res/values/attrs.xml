<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="default_expanded" format="boolean|reference" />

    <declare-styleable name="CollapsiblePreferenceCategory">
        <attr name="default_expanded" />
    </declare-styleable>

    <declare-styleable name="CollapsiblePreferenceCategoryNew">
        <attr name="default_expanded" />
